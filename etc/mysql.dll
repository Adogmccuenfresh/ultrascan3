--
-- Table structure for table 'tblInvestigators'
--

CREATE TABLE tblInvestigators (
  InvID int(11) NOT NULL auto_increment,
  FirstName varchar(30) default NULL,
  LastName varchar(30) default NULL,
  Address varchar(255) default NULL,
  City varchar(30) default NULL,
  State char(2) default NULL,
  Zip varchar(10) default NULL,
  Phone varchar(100) default NULL,
  Email varchar(100) default NULL,
  Organization varchar(50) default NULL,
  PRIMARY KEY  (InvID)
) TYPE=InnoDB;

--
-- Table structure for table 'tblBuffer'
--

CREATE TABLE tblBuffer (
  BuffID int(11) NOT NULL auto_increment,
  InvestigatorID int(11),
  INDEX Invid (InvestigatorID),
  FOREIGN KEY (InvestigatorID) REFERENCES tblInvestigators (InvID) ON DELETE CASCADE,
  BuffFileName varchar(80) NOT NULL default '',
